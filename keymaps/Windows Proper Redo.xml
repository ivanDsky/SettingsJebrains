<keymap version="1" name="Windows Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="AceAction">
    <keyboard-shortcut first-keystroke="ctrl semicolon" />
    <keyboard-shortcut first-keystroke="alt t" />
  </action>
  <action id="AceLineIndentsAction">
    <keyboard-shortcut first-keystroke="shift alt t" />
  </action>
  <action id="AceNextTagAction">
    <keyboard-shortcut first-keystroke="period" />
    <keyboard-shortcut first-keystroke="right" />
  </action>
  <action id="AcePreviousTagAction">
    <keyboard-shortcut first-keystroke="comma" />
    <keyboard-shortcut first-keystroke="left" />
  </action>
  <action id="AceWordAction">
    <keyboard-shortcut first-keystroke="shift ctrl t" />
  </action>
  <action id="AceWordStartAction">
    <keyboard-shortcut first-keystroke="alt s" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="EditorPageDown">
    <keyboard-shortcut first-keystroke="page_down" />
    <keyboard-shortcut first-keystroke="ctrl down" />
  </action>
  <action id="EditorPageUp">
    <keyboard-shortcut first-keystroke="page_up" />
    <keyboard-shortcut first-keystroke="ctrl up" />
  </action>
  <action id="EditorScrollDown" />
  <action id="EditorScrollUp" />
  <action id="GotoTest" />
  <action id="ResourceExplorer.show" />
  <action id="ServiceView.ShowServices" />
  <action id="context.save" />
  <action id="tasks.switch" />
</keymap>